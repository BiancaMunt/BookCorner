{
  "name": "bookcorner",
  "version": "1.0.0",
  "description": "App with client and server side that offers a search for books coming from GoogleBooks API",
  "scripts": {
    "install-client": "cd client && npm install && cd ..",
    "install-server": "cd server && npm install && cd ..",
    "start:client": "cd client && npm start",
    "start:server": "cd server && npm start",
    "start": "npm-run-all --parallel start:server start:client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BiancaMunt/BookCorner.git"
  },
  "author": "Bianca Munteanu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/BiancaMunt/BookCorner/issues"
  },
  "homepage": "https://github.com/BiancaMunt/BookCorner#readme",
  "dependencies": {
    "npm-run-all": "^4.1.5"
  }
}
